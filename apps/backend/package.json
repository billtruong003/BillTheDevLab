{
  "name": "@billthedevlab/backend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/main",
    "migration:generate": "ts-node -O '{\"module\":\"commonjs\",\"emitDecoratorMetadata\":true,\"experimentalDecorators\":true}' -r tsconfig-paths/register ../../node_modules/typeorm/cli.js migration:generate -d src/database/data-source.ts",
    "migration:run": "ts-node -O '{\"module\":\"commonjs\",\"emitDecoratorMetadata\":true,\"experimentalDecorators\":true}' -r tsconfig-paths/register ../../node_modules/typeorm/cli.js migration:run -d src/database/data-source.ts",
    "migration:revert": "ts-node -O '{\"module\":\"commonjs\",\"emitDecoratorMetadata\":true,\"experimentalDecorators\":true}' -r tsconfig-paths/register ../../node_modules/typeorm/cli.js migration:revert -d src/database/data-source.ts",
    "lint": "eslint \"{src,test}/**/*.ts\"",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.650.0",
    "@aws-sdk/s3-request-presigner": "^3.650.0",
    "@nestjs/bullmq": "^10.2.0",
    "@nestjs/common": "^10.4.0",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-fastify": "^10.4.0",
    "@nestjs/schedule": "^4.1.0",
    "@nestjs/typeorm": "^10.0.2",
    "bullmq": "^5.25.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "googleapis": "^144.0.0",
    "ioredis": "^5.4.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.13.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "stripe": "^17.0.0",
    "typeorm": "^0.3.20",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.0",
    "@nestjs/schematics": "^10.2.0",
    "@types/node": "^22.0.0",
    "@types/passport-jwt": "^4.0.1",
    "@types/uuid": "^10.0.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.3"
  }
}
